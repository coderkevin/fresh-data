{
	"name": "@fresh-data/framework",
	"version": "0.7.0",
	"description": "Describe the data you need and simply use it in your app.",
	"license": "MIT",
	"homepage": "https://github.com/coderkevin/fresh-data#readme",
	"repository": "github:coderkevin/fresh-data",
	"bugs": "https://github.com/coderkevin/fresh-data/issues",
	"keywords": [
		"javascript",
		"api",
		"data",
		"functional",
		"reducer",
		"react",
		"redux"
	],
	"author": "Kevin Killingsworth <kk@redfenix.com> (https://github.com/coderkevin)",
	"main": "lib/index.js",
	"module": "es/index.js",
	"files": [
		"dist",
		"es",
		"lib",
		"src"
	],
	"scripts": {
		"clean": "rimraf lib dist es coverage node_modules",
		"build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
		"build:es": "babel src --out-dir es",
		"build:umd": "cross-env NODE_ENV=development rollup -c -o dist/fresh-data-framework.js",
		"build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/fresh-data-framework.min.js",
		"build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
		"lint": "eslint .",
		"test": "jest --verbose --runInBand src",
		"test:watch": "jest --runInBand --watchAll src",
		"prepublishOnly": "npm run clean && npm install && npm run lint && npm test && npm run build"
	},
	"peerDependencies": {
		"debug": "^3.1.0 || ^4.0.0",
		"lodash": "^4.17.10"
	},
	"dependencies": {
		"@babel/runtime": "^7.4.2"
	},
	"devDependencies": {
		"@babel/cli": "7.12.16",
		"@babel/core": "7.12.16",
		"@babel/plugin-proposal-class-properties": "7.12.13",
		"@babel/plugin-proposal-object-rest-spread": "7.12.13",
		"@babel/plugin-transform-async-to-generator": "7.12.13",
		"@babel/plugin-transform-runtime": "7.12.15",
		"@babel/preset-env": "7.12.16",
		"@rollup/plugin-babel": "^5.3.0",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "10.1.0",
		"babel-jest": "26.6.3",
		"codecov": "3.8.1",
		"cross-env": "7.0.3",
		"debug": "4.3.1",
		"eslint": "7.20.0",
		"eslint-config-wordpress": "2.0.0",
		"eslint-plugin-jest": "24.1.5",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-react": "7.22.0",
		"eslint-plugin-wordpress": "git://github.com/WordPress-Coding-Standards/eslint-plugin-wordpress.git",
		"identity-obj-proxy": "3.0.0",
		"jest": "26.6.3",
		"lerna": "3.22.1",
		"lodash": "4.17.20",
		"rimraf": "3.0.2",
		"rollup": "2.39.0",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-replace": "2.2.0",
		"rollup-plugin-terser": "7.0.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"jest": {
		"testURL": "http://localhost",
		"verbose": true,
		"collectCoverage": true,
		"coverageDirectory": "../../coverage",
		"testPathIgnorePatterns": [
			"/node_modules/"
		]
	},
	"gitHead": "77ce7e85b2c5179497bbb98a813b9a9a6c0f852b"
}
